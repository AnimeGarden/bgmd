{
  "name": "bgmd",
  "version": "0.20251018.0",
  "description": "Bangumi data",
  "keywords": [
    "anime",
    "animegarden",
    "bangumi",
    "bgm",
    "tmdb"
  ],
  "homepage": "https://github.com/AnimeGarden/bgmd#readme",
  "bugs": {
    "url": "https://github.com/AnimeGarden/bgmd/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnimeGarden/bgmd.git"
  },
  "license": "MIT",
  "author": "XLor",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./data/index.json",
      "require": "./data/index.json"
    },
    "./full": {
      "types": "./full.d.ts",
      "import": "./data/full.json",
      "require": "./data/full.json"
    },
    "./calendar": {
      "types": "./calendar.d.ts",
      "import": "./data/calendar.json",
      "require": "./data/calendar.json"
    }
  },
  "main": "data/index.json",
  "module": "data/index.json",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "data",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "release": "release-it",
    "test": "vitest",
    "test:ci": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsx": "^4.20.6"
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=v20.8.0"
  }
}
