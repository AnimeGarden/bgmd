    "data": {
      "id": 90502,
      "title": "宇宙兄弟#0",
      "platform": "剧场版",
      "onair_date": "2014-08-09",
      "alias": [
        "Uchuu Kyoudai: Number Zero",
        "宇宙兄弟 剧场版",
