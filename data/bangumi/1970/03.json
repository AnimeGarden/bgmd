      "platform": "剧场版",
      "images": {
        "small": "https://lain.bgm.tv/r/200/pic/cover/l/80/c6/378799_Ov5qg.jpg",
        "grid": "https://lain.bgm.tv/r/100/pic/cover/l/80/c6/378799_Ov5qg.jpg",
