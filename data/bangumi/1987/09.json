        "actors": [
          {
            "images": {
              "small": "https://lain.bgm.tv/r/100/pic/crt/l/48/23/5286_seiyu_anidb.jpg",
              "grid": "https://lain.bgm.tv/pic/crt/g/48/23/5286_seiyu_anidb.jpg",
              "large": "https://lain.bgm.tv/pic/crt/l/48/23/5286_seiyu_anidb.jpg",
